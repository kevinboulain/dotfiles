{
  "^_" = "undo:";
  "^j" = "insertNewline:";
  "^v" = "pageDown:";
  "~<" = "moveToBeginningOfDocument:";
  "~>" = "moveToEndOfDocument:";
  "~b" = "moveWordBackward:";
  "~d" = "deleteWordForward:";
  "~f" = "moveWordForward:";
  "~v" = "pageUp:";
}
